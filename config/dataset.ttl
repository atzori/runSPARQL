@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
@prefix dbo: <http://dbpedia.org/ontology/>
@prefix owl: <http://www.w3.org/2002/07/owl#>

dbo:PopulatedPlace rdfs:subClassOf dbo:Place.
dbo:Settlement rdfs:subClassOf dbo:PopulatedPlace.
dbo:Village rdfs:subClassOf dbo:Place, dbo:Settlement.

dbo:Place rdfs:subClassOf dbo:Location.
dbo:Location rdfs:subClassOf owl:Thing.


dbo:Settlement rdfs:subClassOf dbo:Settlement.

